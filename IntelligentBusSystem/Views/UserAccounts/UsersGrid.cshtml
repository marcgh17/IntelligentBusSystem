@model IEnumerable<IntelligentBusSystem.Models.SUser>
<script src="/Scripts/jquery-1.10.2.min.js"></script>


<!-- Core JavaScript Files -->
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/jquery.dataTables.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        var options = { "iDisplayLength": 3, "aLengthMenu": [[3, 10, 20, -1], [3, 10, 20, "All"]], };
        $('#UsersDT').dataTable(options);

    });
</script>

<table border="0" class="table table-striped table-bordered table-hover" id="UsersDT">
    <thead>
        <tr>
            <th></th>
            <th> <h3><center>Full name</center></h3></th>
            <th><h3> <center>Username</center></h3></th>
            <th><h3> </h3></th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td><img src="@item.SUserPhoto" class="img-rounded" height="50" width="50" /></td>
                <td><h4>@item.SUserFirstName @item.SUserLastName</h4></td>
                <td><h4>@item.SUserID</h4></td>
                <td><a href="">Edit profile</a> </td>
            </tr>
        }

    </tbody>
</table>




