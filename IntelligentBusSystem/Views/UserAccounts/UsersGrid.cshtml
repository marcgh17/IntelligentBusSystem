@model IEnumerable<IntelligentBusSystem.Models.SUser>
<script src="/Scripts/jquery-1.10.2.min.js"></script>


<!-- Core JavaScript Files -->
<script src="/Scripts/jquery.min.js"></script>
<script src="/Scripts/bootstrap.min.js"></script>
<script src="/Scripts/jquery.dataTables.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#UsersDT').dataTable({
            "paging": true,
            "searching": true,
            "ordering": false
        });
        
    });
</script>

<table border="0" class="table table-striped table-bordered table-hover" id="UsersDT">
    <thead>
        <tr>
            
            <th> <h3><center>Full name </center></h3></th><th><h3> <center>Username</center></h3></th>
        </tr>
    </thead>
    <tbody>
      
        @foreach (var item in Model)
        {
            <tr>
                
                <td><h4><img src="@item.SUserPhoto" width="50" height="50" />@item.SUserFirstName @item.SUserLastName</h4></td><td><h4>@item.SUserID</h4></td>
            </tr>
        }
        
    </tbody>
</table>


