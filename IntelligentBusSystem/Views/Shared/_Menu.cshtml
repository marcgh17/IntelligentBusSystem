@model IEnumerable<IntelligentBusSystem.Models.CustomMenuItem>
<ul class="nav navbar-nav">

       @foreach (var Menu in Model)
        {
        if (Menu.subMenus.Count==0)
        {
             <li ><a href="@Menu.Menu.WebAppMenuURL">@Menu.Menu.WebAppMenuText</a></li>
        }
        else
        {
            <li class="dropdown">
                <a href="@Menu.Menu.WebAppMenuURL" class="dropdown-toggle" data-toggle="dropdown">@Menu.Menu.WebAppMenuText<b class="caret"></b></a>
                <ul class="dropdown-menu">
                 @foreach (var SubMenu in Menu.subMenus)
                 { 
                    <li><a href="@SubMenu.WebAppMenuURL">@SubMenu.WebAppMenuText</a></li>
    
                 }
                </ul>
            </li>
        }

        }

 
           

</ul>
